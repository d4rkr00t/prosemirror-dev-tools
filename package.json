{
  "name": "prosemirror-dev-tools",
  "version": "3.0.1",
  "description": "Dev Tools for ProseMirror",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "echo \"No tests yet\"",
    "start": "parcel example/index.html -d ./out",
    "prebuild": "rimraf ./dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm",
    "lint:staged": "lint-staged",
    "pmm:prepare": "npm run build",
    "release:major": "pmm major",
    "release:minor": "pmm minor",
    "release:patch": "pmm patch",
    "ci:github-release": "conventional-github-releaser -p angular"
  },
  "keywords": [],
  "author": "Stanislav Sysoev <@d4rkr00t>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/prosemirror-dev-tools"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion": "^10.0.27",
    "html": "^1.0.0",
    "jsondiffpatch": "^0.4.1",
    "nanoid": "^2.1.11",
    "prop-types": "^15.7.2",
    "prosemirror-model": ">=1.0.0",
    "prosemirror-state": ">=1.0.0",
    "react-dock": "^0.2.4",
    "react-json-tree": "^0.11.2",
    "unstated": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "lint-staged": "^10.2.2",
    "parcel": "^1.12.4",
    "pmm": "^2.0.0",
    "pre-commit": "^1.2.2",
    "preact": "^10.2.2",
    "preact-compat": "^3.19.0",
    "prettier": "^2.0.5",
    "prosemirror-example-setup": "*",
    "prosemirror-schema-basic": "*",
    "prosemirror-view": "*",
    "react": "^16.13.1",
    "react-dom": "^16.4.1",
    "rimraf": "^3.0.2"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
